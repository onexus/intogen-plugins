<collection>
    <title>Mutations</title>
    <loader>
        <plugin>tsv-loader</plugin>
        <parameter>
            <key>data</key>
            <value>/results/gene.tsv</value>
        </parameter>
        <parameter>
            <key>NULL_VALUE</key>
            <value>\N</value>
        </parameter>
    </loader>
    <fields>
        <field>
            <label>geneid</label>
            <title>Geneid</title>
            <id>GENE_ID</id>
            <type>string</type>
            <primary-key>true</primary-key>
        </field>
        <field>
            <label>fm-bias</label>
            <title>fm-bias pValue</title>
            <id>FM_PVALUE</id>
            <type>double</type>
        </field>
        <field>
            <label>fm-bias</label>
            <title>Corrected pValue</title>
            <id>FM_QVALUE</id>
            <type>double</type>
        </field>
        <field>
            <label>clust-pvalue</label>
            <title>OncoCLUST pValue</title>
            <id>CLUST_PVALUE</id>
            <type>double</type>
        </field>
        <field>
            <label>oncoclust</label>
            <title>OncoCLUST corrected pValue</title>
            <id>CLUST_QVALUE</id>
            <type>double</type>
        </field>
    </fields>
    <links>
        <link>
            <collection>${genes}</collection>
            <field>GENE_ID</field>
        </link>
    </links>
</collection>
